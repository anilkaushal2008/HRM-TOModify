@model HRM.Models.tblDeptMas

@if (ViewBag.selectnewdpt != null)
{
    @Html.Label("Update Department", new { @class = "control-label col-md-8" })
    @Html.DropDownListFor(model => model.vchdeptname, ViewBag.selectnewdpt as SelectList, Model.vchdeptname, htmlAttributes: new { @class = "form-control" })
    @Html.ValidationMessageFor(model => model.vchdeptname, "", new { @class = "text-danger" })
}

