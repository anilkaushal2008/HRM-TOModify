@model HRM.Models.tblQualiMas
@if (Session["descript"] != null)
{
    {
        ViewBag.Title = "QualificationMas";
        Layout = "~/Views/Shared/_LayoutMain.cshtml";
        Html.EnableClientValidation(true);
    }

    using (Html.BeginForm("QualificationMas", "Master"))
    {
    @Html.AntiForgeryToken()

     <div class="form-horizontal col-12">
       <br />
       <h4 style="color:black">Qualifaction/Doc Master Setup</h4>
       <hr />
       @Html.ValidationSummary(true, "", new { @class = "text-danger" })

      <div class="form-group">
          @Html.LabelFor(model => model.vchqname, htmlAttributes: new { @class = "control-label col-md-6" })
          <div class="col-md-10">
             @Html.EditorFor(model => model.vchqname, new { htmlAttributes = new { @class = "form-control" } })
             @Html.ValidationMessageFor(model => model.vchqname, "", new { @class = "text-danger" })
          </div>
      </div>
     </div>

    <div class="form-group">
        <div class="col-10">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-dark" />
                @{ string Quali = Session["ViewQualiDocMas"].ToString();
                    if (Quali == "True")
                    {
                        <div align="center">
                            <a href="@Url.Action("ViewAllQualificationMas", "Master")">
                                <div class="btn btn-primary">
                                    <h6 align="center" class="fas fa-hand-point-right">
                                        View All Qualifaction/Doc
                                    </h6>
                                </div>
                            </a>
                        </div>
                    }
                    }
                    </div>
        </div>
    </div>
                        }
                    }
                    else
                    {
                        Html.RenderAction("_SessionError1", "Home");
                    }


