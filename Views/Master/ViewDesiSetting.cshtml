@model IEnumerable<HRM.Models.tblDesiSetting>
@if (Session["descript"] != null)
{
    {
        ViewBag.Title = "ViewDesiSetting";
        Layout = "~/Views/Shared/_LayoutMain.cshtml";
    }

    <div class="col-lg-12">
        <div class="card">
           @if(TempData["Success"] != null)
            {
                <p class="alert alert-success" id="successMessage">@TempData["Success"]</p>
            }
            <div class="card-body">
                <h3 class="card-title">All designation setting master view</h3>
                    <div align="right">
                        <a href="@Url.Action("AddNewSetting", "Master")">
                            <div class="btn btn-primary">
                                <h6 class="fas fa-hand-point-right"></h6>
                                <h6 align="center" class="fa fa-plus">
                                    Create new
                                </h6>
                            </div>
                        </a>
                    </div>
                <div class="table">
                    <table class="table table-sm col-12">
                        <thead class="bg-primary text-white col-12">
                            <tr>
                                @*<th>
                                    Department
                                </th>*@
                                <th>
                                    Designation
                                </th>
                                <th>
                                   Min Experience
                                </th>
                                <th>
                                    Max Experience
                                </th>
                                <th>
                                    Min Salary
                                </th>
                                <th>
                                    Max Salary
                                </th>
                                <th>
                                   DateCreated
                                </th>
                                <th>
                                    CreatedBy
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                        </thead>
                        <tbody class="border border-primary">
                           @if (TempData["Empty"] != null)
                           {
                                <p class="alert alert-danger" id="successMessage">@TempData["Empty"]</p>
                           }
                           else
                           {
                               foreach (var item in Model)
                               {
                        <tr>
                            @*<td>
                                @Html.DisplayFor(modelItem => item.tblDeptMas.vchdeptname)
                            </td>*@
                            <td>
                                @Html.DisplayFor(modelItem => item.tblDesignationMas.vchdesignation)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numExpMin)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numExpMax)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numSalMin)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numSalMax)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.dtcreated)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.vchCreatedBy)
                            </td>
                            <td>
                                <a href="@Url.Action("EditSetting", "Master", new { id = item.intid })">
                                    <h6 align="center" class="fa fa-edit" data-placement="left" data-toggle="tooltip" data-original-title="Edit"></h6>
                                </a>&nbsp;&nbsp;
                                <a href="@Url.Action("DeleteQualiSetting", "Master", new { id = item.intid })">
                                    <h6 align="center" class="fa fa-trash" data-placement="right" data-toggle="tooltip" data-original-title="Delete"></h6>
                                </a>
                            </td>
                        </tr>
                               }
                           }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
}
else
{
    Html.RenderAction("_SessionError1", "Home");
}

